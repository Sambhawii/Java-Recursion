public class RPrime {
    public static boolean isPrime(int num,int i){
        if(i<num/2){
        if(num%i==0){
            return false;
        }
        return isPrime(num, i+1);
    }
        return true;
    }
    public static void main(String[] args) {
        int num =1182;
        boolean ans =isPrime(num,2);
        System.out.println(ans);
    }
}
